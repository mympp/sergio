			.bbs-wrapper {
				position: relative;
				z-index: 1;
				height: 51px;
				width: 100%;
				background: #fff;
				overflow: hidden;
				-ms-touch-action: none;
			}
			.bbs-wrapper:after {
				border-bottom: 1px solid #d7d7d7;
				content: "";
				position: absolute;
				bottom: 0px;
				left: 0px;
				right: 0px;
				-webkit-transform: scaleY(.5);
				-webkit-transform-origin: 0 0;
			}
			.menu-bar {
				position: absolute;
				z-index: 1;
				height: 50px;
			}
			.menu-bar ul {
				width: 100%;
				display: -webkit-box;
				-webkit-box-orient: horizontal;
				box-sizing: border-box;
				-webkit-box-sizing: border-box;
				position: relative;
			}
			.menu-bar li {
				min-width: 90px;
				padding:0 0.35rem;
				height: 50px;
				line-height:normal;
				font-size: 16px;
				overflow: hidden;
				text-align: center;
				color: #000;
				box-sizing: border-box;
				-webkit-box-sizing: border-box;
				display: -webkit-box;
				-webkit-box-orient: horizontal;
				-webkit-box-pack: center;
				-webkit-box-align: center;
				display: box;
				box-orient: horizontal;
				box-pack: center;
				box-align: center;
			}
			.menu-bar li a { color:#666;}
			.menu-bar li .aui-badge-info{
				background: #E1E1E1;
			}

			.menu-bar li.active {
				color: #3498db !important;
				border-bottom: 2px solid #3498db;
			}
			.menu-bar li.active a { color: #000; }
			.menu-bar li.active .aui-badge-info{
				background:#3498db;
			}
