            });
            if (productIds.length > 0) {
                //延迟执行
                window.setTimeout(function () {
                    //获取json结果集合
                    $.post("/api/aspx/shopping.aspx?cmd=99", { productids: productIds.join(',') }, function (dat) {
                        if (dat == null || dat.length <= 0) {
                            //没有数据时执行
                        } else {
                            //有数据时执行
                            var dats_data = dat;
                            for (var x in dats_data) {
                                var type = dats_data[x].OfferType;
                                var minmoney = 0.0;
                                var maxmoney = 0.0;
                                if (type == 2) {
                                    //按规格价
                                    var guigeshuxin = dats_data[x].Specifications;
                                    for (var y in guigeshuxin) {
                                        if (minmoney == 0 || minmoney >= guigeshuxin[y].danjia) { minmoney = guigeshuxin[y].danjia; }
                                        if (maxmoney <= guigeshuxin[y].danjia) { maxmoney = guigeshuxin[y].danjia; }
                                    }
                                } else {
                                    //按阶梯价
                                    maxmoney = dats_data[x].PriceDetail.danjia1;
                                    minmoney = dats_data[x].PriceDetail.danjia1;
                                    if (dats_data[x].PriceDetail.num3 > 0) {
                                        minmoney = dats_data[x].PriceDetail.danjia3;
                                    } else if (dats_data[x].PriceDetail.num2 > 0) {
                                        minmoney = dats_data[x].PriceDetail.danjia2;
                                    }
                                }
                                if (minmoney > 0) {
                                    $("#productid_" + dats_data[x].businessid).html("￥" + parseFloat(minmoney).toFixed(2));
                                }
                            }
                        }
                        $(".hot-prod-price").each(function () {
                            if ($(this).html() == "￥" || $(this).html() == "￥0") {
                                $(this).html("价格:电询");
                            }
                        });
