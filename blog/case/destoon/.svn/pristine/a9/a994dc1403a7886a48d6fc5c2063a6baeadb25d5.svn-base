            //分类鼠标移上
            categoriesHover:function(){
                $('.categories-l a').hover(function(){
                    $(this).children('.categories-bg').stop().animate({top:"0"},"slow");
                    $(this).children('.categories-t1').stop().animate({top:"145px"},"slow");
                    $(this).children('.categories-t2').stop().animate({top:"185px"},"slow");
                },function(){
                    $(this).children('.categories-bg').stop().animate({top:"315px"},"slow");
                    $(this).children('.categories-t1').stop().animate({top:"316px"},"slow");
                    $(this).children('.categories-t2').stop().animate({top:"360px"},"slow");
                })
            
            },
            orderHover:function(){
                $('.order1 li').hover(function(){
                    $(this).children('.orderHover').show();
                },function(){
                    $(this).children('.orderHover').hide();
                })
