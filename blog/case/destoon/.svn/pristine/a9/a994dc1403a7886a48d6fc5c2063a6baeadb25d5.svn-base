            changeHeight:function(){
                $('.orderCont .bd .order2 .title a').each(function(){
                    var titleheight = $(this).height();
                    if(titleheight>45){
                        $(this).parents().next().children('.txt').css({'line-height':'18px','height':'18px'});
                        $(this).parents().next('.cont').css({'padding':'5px 0'});
                    }
                });
                $('.orderCont .bd .order1 .title a').each(function(){
                    var titleheight = $(this).height();
                    if(titleheight>45){
                        $(this).parents().next().children('.txt').css({'line-height':'20px','height':'20px'});
                        $(this).parents().next('.cont').css({'padding':'5px 0'});
                    }
                })
