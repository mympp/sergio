/*焦点图片*/
.machine-scroll-img {
	height: 260px
}
.machine-scroll-img #ifocus {
	width: 470px; height: 260px;
}
.machine-scroll-img #ifocus_pic {
	position: relative; margin: 0px; width: 470px; height: 260px; overflow: hidden; z-index:2;
}
.machine-scroll-img #ifocus_piclist {
	position: absolute; z-index: 5;
}
.machine-scroll-img #ifocus_piclist li {
	width: 470px; height: 260px; overflow: hidden
}
.machine-scroll-img #ifocus_piclist img {
	width: 470px; height: 260px
}
.machine-scroll-img #ifocus_btn {
	z-index: 6; position: absolute; margin: 0px; bottom: 4px; display: inline; right: 0px;
}
.machine-scroll-img #ifocus_btn li {
	border: #fff 1px solid; text-align: center; filter: alpha(opacity=90); line-height: 16px; width: 18px; display: inline; background: #86b615; float: left; height: 16px; color: #fff; font-size: 14px; cursor: pointer; font-weight: bold; margin-right: 6px;  opacity: 0.9; -moz-opacity: 0.9
}
.machine-scroll-img #ifocus_btn .current {
	border: #fff 1px solid; filter: alpha(opacity=100); background: #f60; color: #fff; opacity: 1; -moz-opacity: 1;
}
.machine-scroll-img #ifocus_opdiv {
	position: absolute; filter: alpha(opacity=50); width: 470px; bottom: 0px; background: #000; height: 30px; left: 0px; opacity: 0.2; -moz-opacity: 0.2
}
.machine-scroll-img #ifocus_tx {
	z-index: 0; position: absolute; line-height: 20px; padding-left: 14px; width: 470px; bottom: 4px; height: 20px; color: #fff; font-weight: bold; left: 0px; 
}
.machine-scroll-img #ifocus_tx .normal {
	display: none;
}
#ifocus_tx .normal{
	font-family:"microsoft yahie"; color:#fff;
}
#ifocus_tx .current{
	font-family:"microsoft yahei"; color:#fff;
}

