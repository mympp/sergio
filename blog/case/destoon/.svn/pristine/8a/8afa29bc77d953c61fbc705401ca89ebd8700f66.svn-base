#goback a:hover{background:#cdcdcd url(image/index_201311.gif) no-repeat -230px -149px;}
#close{display:inline-block; position:absolute;right:-13px;top:0;width:13px;height:14px;background-position:-380px -90px;cursor:pointer;}

