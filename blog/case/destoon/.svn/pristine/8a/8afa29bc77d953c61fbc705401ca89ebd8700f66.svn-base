/*右侧浮动导航*/
.right-nav li a:hover,.right-nav li.cur,#goback a,#close{background:url(image/index_201311.gif) no-repeat;}
.right-nav{position:fixed;left:50%;margin-left:510px;top:100px;display:none;_position:absolute;_bottom:auto;_top:expression(eval(document.documentElement.scrollTop)+36);}
.right-nav ul{background:url(image/right_nav_bg.jpg) repeat-y top left;width:120px;border-top:solid 1px #e1e1e1;}
.right-nav li{line-height:30px;height:30px;color:#656565;vertical-align:middle;overflow:hidden;}
.right-nav li a{display:block;color:#656565;height:30px;cursor:pointer;vertical-align:top;}
.right-nav li.cur a{display:block;color:#fff;}
.right-nav li a:hover{background-position:-230px -90px;color:#fff;height:30px;}
.right-nav li.cur{background-position:-230px -90px;color:#fff;}
.right-nav li span{float:left;display:block;height:30px;line-height:30px;}
.right-nav li span.floor-level{width:30px;padding-left:15px;}
.right-nav li span.floor-name{width:75px;}
#goback{border:solid 1px #e1e1e1;height:26px;width:118px;}
