.feedback-box{ width: 298px; top: auto; bottom: 0;right: 45px; background:#fff; position:absolute; display:none;}
.feedback-ctn{ padding: 14px 20px;}
.feedback-h{ margin-bottom: 5px; font-size: 14px; font-weight: 700;}
.feedback-b{ margin-bottom: 5px;}
.feedback-wrap{ border: 1px solid #ddd; padding: 6px 6px 20px; position:relative;}
.feedback-textarea{ position: relative; width: 100%; height: 128px; padding: 0; background-color: transparent; overflow-y: auto;
    border: 0; outline: 0; resize: none; z-index: 100;}
.feedback-ph{ position: absolute; top: 6px; left: 4px; color: #ddd;}
.feedback-f{ text-align: right; position:relative;}
.feedback-f span{ color: #cf1f3f;}
.feedback-f p{ top: -25px; right: 5px; color: #969696; position:absolute;}
.feedback-btn{ width: 64px; height: 22px; line-height: 22px; border-radius: 2px; display: inline-block;  background:#ff500b; color:#fff; text-align:center; line-height:22px;}
.feedback-btn:hover{ color:#fff;}
.feedback-close{ font-size: 12px; font-family: 'SimSun'; color: #333; top: 14px; right: 20px; position: absolute; }












/*****右侧浮动样式*****/

/****右侧浮动begin*****/
.zhiding { position: fixed; bottom:60px;right:1px; _position: absolute; _top: expression(documentElement.scrollTop+documentElement.clientHeight-this.offsetHeight); _margin-bottom:60px;
    width: 62px; z-index: 10000; border:solid 1px #dbdbdb; background:#f7f7f7; }
a.erwei,a.weibo,a.ztop{color:#fff; font-size:14px; line-height:20px; display:block;border-top:solid 1px #dbdbdb; width:62px; height:52px; position:relative;}
a.erwei{ background: url('image/ico_bling2.png') no-repeat 0px -55px;}
a.weibo{ background: url('image/ico_bling2.png') no-repeat 0px -105px;}
a.ztop{ display:none; background: url('image/ico_float.gif') no-repeat 0px -160px;}
.erwei .mama{ display:none;left: -112px;top:-46px;color:#a5a5a5; position: absolute; border:solid 1px #ddd; background:#fff;}
.erwei .saomiao{ text-align:center; padding-bottom:5px; font-size:13px;}
.weibo #weibo{ display:none; z-index: -1; position: absolute; right: 63px;top: 0px;padding: 6px; background:#fff;}
.zhiding .hover_t{ background:#ff510c; padding:5px 17px; height: 42px; width: 28px; display:none; position:absolute; top:0; left:0; color:#fff; cursor:pointer;}
.minichat_shouq { display: block; height: 52px;overflow: hidden; cursor: pointer; position:relative; }
.minichat_shouq .zaixiankf{ background: url('image/ico-kefu.png') no-repeat 0px 0px; position: absolute; top: 0; left: 0;
     height: 42px; width: 28px; padding:5px 17px; color:#fff; font-size:14px; line-height:20px;}
.minichat_shouq  input{ opacity: 0; border: none; display: inline-block; background: none;}
.ycfdHyfw {background:url('image/pgycIcon.gif') no-repeat 50% 5px #ed0000;display: block; height: 52px;overflow: hidden; cursor: pointer; position:relative;}
.ycfdHyfw  .ycfdHyfwbtu {display:block; background: none;color:#fff;height:100%;width:100%;line-height:82px;text-align:center;cursor:pointer;}
/****右侧浮动end*****/



/******面包屑*****/
.crumbs{ line-height:36px; padding-top:2px; overflow:hidden;}
.crumbs a{ color:#222; display:block; float:left;}
.crumbs a:hover{ color:#ff510c;}
.crumbs .corn{ font-family:sans-serif; color:#222; padding:0 4px; display:block; float:left;}
.crumbs span{ color:#222; display:block; float:left}
.crumbs h1{ display:initial; font-size:12px; color:#222; display:block; float:left;}


