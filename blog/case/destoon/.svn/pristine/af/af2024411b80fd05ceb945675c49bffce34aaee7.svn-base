body{margin:0;padding:0;}
ul,li{list-style:none;margin:0;padding:0;}
html,body,div,dl,dt,dd,ul,h1,h2,h3,pre,form,label,fieldset,p,blockquote,table{font-size:12px;margin:0;padding:0; font-family:Arial,Helvetica, sans-serif ;} 
a {text-decoration: none; color: #333;}
.m{width:1200px;margin:0 auto;}
.head-wrap{width:100%;height:50px;box-shadow: 1px 2px 2px #d3d3d3;}
