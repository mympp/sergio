.search{float:left;width:965px;padding-left:20px;}


.tjcp .hover {position: absolute;left: 0px;top: 0px;width: 240px;height: 200px;background: url('image/OrigePng1px.png') repeat;}
.hover ul{overflow:hidden;margin:0;padding:0;}
.hover ul li{text-align:center;line-height:200px;font-size:18px;font-weight:bold;color:#fff;}
.hover ul li a:link{text-align:center;line-height:164px;font-size:18px;font-weight:bold;color:#fff;text-decoration:none;}
.hover ul li a:hover{color:#fff;}

