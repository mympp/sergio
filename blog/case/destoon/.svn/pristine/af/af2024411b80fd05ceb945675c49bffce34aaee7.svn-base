.listview a:link{width:80%;color: #333333;text-decoration:none;display:block;}
.listview a:hover{display:block;}
.cur{display:block;float: left;width: 100%;padding:10px 0 10px 10px;line-height: 20px;font-size:14px;background:#f6f5f5;border:1px solid #e5e5e5;border-right:none;color:#ed6300;}
.t1{background:none;border:none;font-size:18px;color:#888;text-decoration:none;display:block;padding-top:20px;}

