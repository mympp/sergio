.pro_serve a, .category li a { color: #666; text-decoration: none; }
.moL a:hover, .moR a:hover, .pro_serve a:hover, .category_cite dd a:hover { color: #f40; text-decoration: underline; }
.pro_serve li.mar_r0 { margin-right: 0; }
.category, .category_cite, .pro_serve, .phone3G { border: #dadada 1px solid; background-color: #fff; }
.topnav span a, .category li.fort, .category li.fort h2 { position: relative; }
.category li.fort h2 a, .category li h2 a:hover, .category_cite dt a { font-weight: bold; }
.category a, .pro_serve a, .phone3G a { background: none; padding: 0; }
.newR { padding-right: 12px; display: inline-block; zoom: 1; position: relative; top: 0; left: -20px; *display:inline;
/*background:url(images/mo_line.png) left 9px no-repeat;*/}
.phone { position: relative; left: -7px; margin-right: 8px; display: inline-block; *display:inline;
}
