.menu_on a{color:#fff;}

.m1{width:100%;height:400px;}
.side_head {height:40px;line-height:40px;background:#6E6E6E;/*border:#99C4D5 1px solid;border-bottom:none;background:url('side_head.gif') repeat-x;*/}
