body {margin:0px;font-size:12px;color:#000000;} /*background:#f6f6f6;*/
/* common */
.m {width:1200px;margin:0 auto;}
.top {width:500px;float:left;height:34px;line-height:34px;color:#949494;}
.container{width:100%;height:34px;line-height:34px;font-size:12px;background:#000;}
