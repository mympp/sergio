.k1{margin-top: 10px; padding: 10px 10px; box-shadow: 0 0 8px 1px #D0D1D2;overflow: hidden;}
/**********商学院**************/
.business{width:840px; float:left;}
.business370{ width:370px;}
.business_left ul{ width:370px;}
.business_left ul li{ width:175px; float:left; margin:0 10px 0 0;  }
.business_left ul li img{ width:175px; height:117px; vertical-align:top}
.business_left ul li b{ display:block; width:100%; height:30px; line-height:30px; text-align:center; background:#e5e5e5; font-weight:normal;white-space: nowrap; text-overflow: ellipsis; overflow: hidden; margin-bottom:5px;}
.business_c{ width:450px;}
.change_left{ width:50px; float:left}
.change_left ul li{ width:100%; float:left; height:100px; line-height:100px; font-size:15px; text-align:center; background:#E6E6E6; cursor:pointer; margin-bottom:1px;}
.change_left ul li.on{ background:#047C32; height:99px; line-height:99px; margin-bottom:1px; color:#FFFFFF; font-weight:bold}
.change_right{ width:380px; float:right; display:none;margin-top:-10px;}
.change_right ul li{ width:100%;height:32px; float:left; line-height:32px; font-size:15px;}
.change_right ul li a{float:left;display:block; width:315px;height:32px;line-height:32px; font-size:15px;white-space: nowrap; text-overflow: ellipsis; overflow: hidden;}
.fl{ float:left;}
.ml10{ margin-left:10px;}
/**商学院右侧**/
.business_right{ overflow: hidden;width:330px; float: left; margin-left:30px;margin-top:5px;}
.business_right ul{ margin-top:10px;}
.business_right li a{text-decoration:none; }
.business_right li{display: inline-block;  height: 32px; line-height: 32px; padding-left: 10px;font-size:15px;}

.business_right i{font:16px "Georgia";margin-right:10px;}
.business_right i.red{color:#DF3B0A;}
.business_right i.bold{font-weight:bold;}
.content_right_buss{border-bottom:2px solid #047C32; height:35px;margin-left:10px; margin-right:10px;}
.content_right_buss h2{line-height:30px; height:30px; font-size:18px; color:#047C32; font-weight:bold; border-bottom:2px solid #047C32; float:left; padding-bottom:5px;letter-spacing:1px;}
.content_right_buss .tltle_span{display: block;}
/*一键加盟，全程无忧CSS*/
.large-topic15{ background: url(http://www.jcqm001.com/img/500.jpg) top center no-repeat; height: 500px;} 
.large-img,.small-img{ width: 100%; height: 100%; display:block;}
.large-cont{ width: 1000px; margin: 0 auto; height: 5px; position: relative; }
.large-cont .lag-cloes{ width: 60px; height: 60px; display: block; position: absolute; top: -474px; right: 3px; cursor:pointer; }
.large-topic15,.small-topic15{ width: 100%; position: relative; display:none;}
.small-topic15{background: url(http://www.jcqm001.com/img/70.gif) top center no-repeat;height: 70px;} 
.small-cont{ width: 1000px; margin: 0 auto; height: 2px; position: relative; }
.small-cont .small-cloes{ width: 50px; height: 28px; display: block; position: absolute; top: -65px; right: 40px; cursor:pointer; }

.hits{color:#999999; font-weight:normal;padding-left:30px; background:url(./image/eye.png) no-repeat scroll 4px 3px transparent;}












