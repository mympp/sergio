/*悬浮框*/
.tooltip{ width:36px; position:fixed; right:0; top:0; bottom:0; background:#000; z-index:9999999;}
.tooltip ul.tool1{ margin-top:-120px; position:absolute; top:50%;}
.tooltip ul.tool2{ position:absolute; bottom:0;}
.tooltip li{ margin:3px 0; position:relative; float:left;}
.tooltip li.on .tool_nav{ background:#d00;}
.tooltip .tool_nav{ width:36px; height:36px; position:relative;}
.tooltip .tool_nav a{ width:100%; height:100%; display:block;}
.tooltip .tool_nav i{ width:20px; height:20px; margin:8px 0 0 8px; background:url(../assets/web/images/tooltip_icon.png); border:none; float:left; line-height:0; font-size:0; text-indent:-99999px;}
