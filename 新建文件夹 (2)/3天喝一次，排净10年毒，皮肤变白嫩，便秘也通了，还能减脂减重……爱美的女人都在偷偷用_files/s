            insertAfter(mydiv,a);
            a.style.cssText += ";width: " + obj.w + "px !important;height: " + obj.h + "px !important;";
            a.setAttribute("width",obj.w);
            a.setAttribute("height",obj.h);
            a.setAttribute("data-vh",obj.vh);
            a.setAttribute("data-vw",obj.vw);
