	font-weight: bold;
}
.page-set .big-img{
	width: 74px;
	height: 100px;
	cursor:pointer;
}
.page-set .bbig-img{
	width: 300px;
	height: auto;
	cursor:pointer;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%,-50%);
	-o-transform: translate(-50%,-50%);
	-ms-transform: translate(-50%,-50%);
	-moz-transform: translate(-50%,-50%);
	-webkit-transform: translate(-50%,-50%);
	z-index: 99999;
}