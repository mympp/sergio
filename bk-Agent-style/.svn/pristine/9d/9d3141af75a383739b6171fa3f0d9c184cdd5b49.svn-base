/* CSS Document 
-功能：代理商系统角色管理模块
-作者：sergiolin
-日期：2017/11/07*/
@charset "utf-8";
/*公共部分*/
.page-set select{
	margin-left: 0px;
	border: solid 1px #000;
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
	background: url("../Images/arrow.png") no-repeat scroll #fff;
	background-position: 97% 50%;
	padding-right: 14px;
}
.page-set select::-ms-expand { display: none; }
.page-set option{
	height: 22px;
    line-height: 22px;
}
.page-set{
	margin: 20px 20px 20px 285px;
	box-sizing: border-box;
}
.page-set .cb{
	clear: both;
}
.page-set .vit-top{
	vertical-align: top;
}
.page-set .white-bg{
	padding: 0px 20px;
	background: #fff;
	border: 1px solid #fff;
	border-radius: 8px;
}
.page-set .grid .gray{
	color: #dcdcdc;
}
.page-set h2{
	font-size: 16px;
	color: #494949;
	font-weight: bold;
	text-indent: 25px;
	margin-top: 22px;
}
.page-set .blue-btn,
.page-set .white-btn{
	width: 90px;
	height: 32px;
	line-height: 30px;
	display: inline-block;
	border-radius: 5px;
	text-align: center;
	font-size: 14px;
	cursor: pointer;
}
.page-set a.blue-btn,
.page-set a.white-btn{
	line-height: 34px;
}
.page-set .blue-btn{
	color: #fff;
	border: 1px solid #2e86e0;
	background: #1da1fe;
}
.page-set .blue-btn:active{
	background: rgba(29,161,242,.8);
}
.page-set .white-btn{
	color: #494949;
	border: 1px solid #cecece;
	background: #e4e6e8;
	margin-left: 58px;
}
.page-set .white-btn:active{
	background: #dbdbdb;
}
.page-set .blue-btn.fr{
	margin-right: 2%
}
.page-set .moni-box{
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0;
	left: 0;
	background: rgba(0,0,0,0.3);
	z-index: 999;
}
.page-set .moni-lbox{
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%,-50%);
	-o-transform: translate(-50%,-50%);
	-ms-transform: translate(-50%,-50%);
	-moz-transform: translate(-50%,-50%);
	-webkit-transform: translate(-50%,-50%);
	z-index: 9999;
}
.page-set .moni-lbox .moni-head{
	width: 100%;
	height: 44px;
	background: #1da1f2;
	border-radius: 5px 5px 0 0;
}
.page-set .moni-main{
	border-radius: 0 0 5px 5px;
	background: #fff;
}
.page-set .moni-lbox .moni-head p{
	line-height: 44px;
	color: #fff;
	font-size: 18px;
	text-indent: 22px;
}
.page-set .moni-lbox .moni-head a{
	line-height: 44px;
	float: right;
	color: #fff;
}
.page-set .moni-lbox .moni-main{
	padding: 28px 58px 30px;
}
.page-set td a{
	width: 50%;
	color: #1da1f2;
	text-align: center;
}
.page-set .tda2 a{
	width: 50%;
}
.page-set .tda3 a{
	width: 33.3%;
}
.page-set .moni-main textarea{
	width: 286px;
	height: 104px;
	padding-left: 20px;
	margin-bottom: 10px;
	border: 1px solid #d0d0d0;
	background: #f5f5f5;
}
.page-set .nopassAgent-box,
.page-set .passAgent-box,
.page-set .stop-box,
.page-set .send-clean,
.page-set .remove-box{
    width: 350px;
    height: 200px;
    background: #fff;
    text-align: center;
    border-radius: 5px;
}
.page-set .nopassAgent-box p,
.page-set .passAgent-box p,
.page-set .stop-box p,
.page-set .send-clean p,
.page-set .remove-box p{
    font-size: 18px;
    color: #494949;
    font-weight: bold;
    margin-top: 60px;
}
.page-set .btn-box{
	width: 100%;
	margin: 25px auto 0;
	text-align: center;
}
.page-set .btn-box .white-btn{
	margin-left: 26px;
}
.page-set .DataCenterBox-cols{

    cursor: pointer;
    font-size: 40px;
    float: right;
    margin-right: 20px;
    margin-top: -5px;
    color: #FFFFFF;
}
/*input内文字样式*/
.page-set ::-webkit-input-placeholder{
	color: #898989;
	font-size: 14px;
}
.page-set ::-moz-placeholder{/*firefox 19+*/
	color: #898989;
	font-size: 14px;
}
.page-set :-ms-input-placeholder{/*ie*/
	color: #898989;
	font-size: 14px;
}
.page-set input:-moz-placeholder{
	color: #898989;
	font-size: 14px;
}
/*表格样式开始*/
.page-set .table-wrap {
    float: left;
    width: 96%;
    margin: 15px 2% 0;
    position: relative;
    border-radius: 5px;
    box-shadow: 0px 2px 12px #e3e3e3;
    overflow: hidden;
}
.page-set .table-head {
	background-color: #54b4ee;
}
.page-set .table-head-wrap {
	width: 100%;
	position: relative;
	overflow: hidden;
}
.page-set .grid {
	margin: 0px;
	table-layout: fixed;
	width: 100%;
	color: #fff;
	max-width: none;
	border-spacing: 0px;
	empty-cells: show;
	border-width: 0px;
	outline: 0px none;
	border-collapse: collapse;
}
.page-set .grid tr th {
	padding: 10px 8px;
	vertical-align: bottom;
	overflow: hidden;
	font-weight: normal;
	white-space: nowrap;
	text-overflow: ellipsis;
	text-align: center;
}
.page-set .grid tr td {
	padding: 10px 8px;
	font-size: 13px;
	color: #898989;
	overflow: hidden;
	line-height: 1.6em;
	vertical-align: middle;
	text-overflow: ellipsis;
	text-align: center;
}
.page-set .table_ctbody tr {
	border-bottom: 1px solid #ececec;
}
.page-set .tab-link {
	display: block;
	font-size: 13px;
	color: #ffffff;
	min-height: 18px;
	line-height: 18px;
	overflow: hidden;
	text-overflow: ellipsis;
}
.page-set .table-content {
    position: relative;
    width: 100%;
    white-space: nowrap;
    overflow-x: hidden;
    overflow-y: auto;
    background: #fff;
}
/*表格样式结束*/
.page-set .table-content1{
	overflow: auto;
	overflow-x: hidden;
}
