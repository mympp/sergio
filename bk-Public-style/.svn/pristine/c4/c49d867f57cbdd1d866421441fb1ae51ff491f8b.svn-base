.about-title{
    width:100%;
    text-align:center;
}
.about-title1{
    margin: 100px 0 120px;
}
.about-title2{
    margin: 208px 0 120px;
}
.about-title3{
    margin: 60px 0 60px;
}
.about-title4{
    padding: 50px 0 56px;
}
.about-title-box{
	width: 460px;
	margin: 0 auto;
	border-bottom: 1px solid #494949;
}
.about-title h2{
    font-size: 36px;
	padding-bottom: 30px;
}
.about-title .about-title-lbox{
	width: 76px;
	border-bottom: 4px solid #352e2b;
	margin: 0 auto;
}

/*联系我们开始*/
.contact_us{
    width: 1200px;
    margin: 0 auto;
    background: #fff;
    box-shadow: 1px 1px 3px 2px rgba(0,0,0,.08);
    position: relative;
    top: -50px;
    padding-bottom: 50px;
}

.contact_us .index-title1{
	padding-top: 30px;
	text-align: center;
}

.contact_us_cell{
	width: 100%;
	height: 654px;
    background: #f7f7f7;
    margin: 0 auto;
}
.contact_us_items{
	width: 100%;
    margin: 25px auto 0;
}

.about-map{
    width: 100%;
    margin: 25px auto 0;
}
.about-mapleft{
    width: 50%;
    height: 436px;
    float: left;
    font-size: 0;
}
.about-mapleft #allmap{
	width: 100%;
	height: 436px;
}

.about-mapright{
	width: 50%;
	height: 436px;
}
.about-mapright-lbox{
	margin-left: 60px;
	line-height: 32px;
	margin-bottom: 5px;
}
.about-mapright-lbox img{
	width: 25px;
	vertical-align: middle;
	position: relative;
	top: -3px;
	display: inline-block;
	margin-right: 10px;
}
.about-mapright-lbox span{
	font-size: 18px;
	color: #898989;
}
.about-mapright-lbox i{
	font-style: normal;
	margin-right: 20px;
}
.about-mapright-lbox label,
form .about-mapright-lbox label{
	font-size: 18px;
	color: #494949;
}
form .about-mapright-lbox label{
	display: inline-block;
	width: 80px;
	margin-left: 20px;	
}
form .about-mapright-lbox input{
	width: 25.7%;
	height: 42px;
	border: 1px solid #ccc;
	background: #fff;
	margin-top: 10px;
	margin-bottom: 20px;
	box-sizing: border-box;
}
form .textarea-label{
	vertical-align: top;
}
form .about-mapright-lbox textarea{
	width: 67.8%;
	height: 120px;
	background: #fff;
	border: 1px solid #ccc;
	resize : none;
}
form a{
	display: inline-block;
	width: 126px;
	height: 42px;
	margin-left: 48%;
	line-height: 40px;
	text-align: center;
	background: #f07122;
	font-size: 16px;
	color: #fff;
}
/*加入我们结束*/
/*联系我们结束*/
/*加盟页面开始*/
