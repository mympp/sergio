/*页面顶部开始*/
.header {
	width: 100%;
	height: 43px;
	border-bottom: 1px solid #EBEBEB;
}

.container {
	margin: 0 auto;
	width: 1200px;
	height: 43px;
}

.container div a {
	display: block;
	padding: 1px 0;
}

.container ul li a {
	display: block;
	padding-right: 15px;
	height: 43px;
	line-height: 43px;
}
/*页面顶部结束*/

/*页面底部开始*/
#wrapper {
	position: relative;
	min-width: 1200px;
	height: 100%;
	min-height: 700px;
}

.erweima {
	position: absolute;
	bottom: 110px;
	text-align: center;
	width: 100%;
}

.footer {
	position: absolute;
	bottom: 10px;
	text-align: center;
	width: 100%;
}

.footer p {
	color: #666;
	font-size: 14px;
}

.footer p a {
	color: #666;
}

.footer p a:hover {
	color: #ff0700;
}

.footer p:nth-child(2) {
	margin-top: 10px;
}

.footer p span {
	padding: 0 5px;
}

.kf-rx {
	display: block;
	margin-top: 5px;
}
/*页面底部结束*/

/*关于雀搜开始*/
